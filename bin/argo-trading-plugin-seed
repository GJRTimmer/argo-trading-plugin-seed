#!/usr/bin/env node

"use strict";

var util = require("util"),
    seneca = require("seneca");

seneca().client({host: "localhost"}).act("role:plugin", {
        cmd: "register",
        name: "argo-trading-plugin-seed"
    }, function (err, result) {
        if (!err) {
            util.log("Argo plugin:", result.value);
        }
    });
